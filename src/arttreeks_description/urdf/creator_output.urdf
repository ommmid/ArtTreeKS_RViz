<?xml version="1.0" ?>
<robot name="atks" xmlns:xacro="http://ros.org/wiki/xacro">
  <link name="base"/>
  <link name="tool0">
      <visual>
       <geometry>
         <cylinder length="0.6" radius="0.2"/>
        </geometry>
      </visual>
  </link>


  <joint name="base_to_tool0" type="fixed">
    <parent link="base"/>
    <child link="tool0"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
  </joint>
</robot>
